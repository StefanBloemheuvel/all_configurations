        // "panel.background": "#FF4500", // Background color of the panel
        "panel.border": "#FF4500", // Border color of the panel
        "panel.dropBackground": "#282828", // Background color when dragging and dropping an item into the panel
        "panelTitle.activeBorder": "#FFD700", // Border color of the active panel title
        "panelTitle.activeForeground": "#FFFFFF", // Foreground color of the active panel title
        "panelTitle.inactiveForeground": "#A5A5A5", // Foreground color of ina

        "sideBar.border": "#FF4500"
    },
    "workbench.colorTheme": "Atom One Dark"
